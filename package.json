{
  "name": "timesink",
  "private": true,
  "workspaces": [
    "backend",
    "frontend",
    "prisma"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:backend": "npm run dev --workspace=backend",
    "prisma:generate": "npm run prisma:generate --workspace=@timesink/prisma"
  },
  "devDependencies": {
    "typescript": "~5.8.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "@types/node": "^24.1.0",
    "@types/express": "^5.0.3",
    "@types/cookie-parser": "^1.4.9",
    "@types/jsonwebtoken": "^9.0.10",
    "prisma": "^6.13.0",
    "concurrently": "^8.2.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "tsx": "^4.20.3",
    "eslint": "^9.30.1",
    "@eslint/js": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "vite": "^7.0.4",
    "@vitejs/plugin-react": "^4.6.0",
    "tailwindcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "axios": "^1.11.0",
    "react-router-dom": "^7.7.1",
    "@tanstack/react-query": "^5.85.5"
  }
}
